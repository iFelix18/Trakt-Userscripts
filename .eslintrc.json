{
  "env": {
    "browser": true,
    "es2022": true
  },
  "extends": [
    "standard",
    "greasemonkey",
    "plugin:jsdoc/recommended",
    "plugin:json/recommended",
    "plugin:regexp/recommended",
    "plugin:unicorn/recommended"
  ],
  "parserOptions": {
    "ecmaVersion": "latest",
    "sourceType": "module"
  },
  "plugins": [
    "jsdoc",
    "json",
    "regexp",
    "unicorn"
  ],
  "rules": {},
  "overrides": [
    {
      "files": "**/*.user.js",
      "extends": [
        "plugin:userscripts/recommended"
      ],
      "plugins": [
        "userscripts"
      ]
    }
  ]
}
